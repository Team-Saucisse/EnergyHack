@{
    ViewBag.Title = "Accueil";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}
<ol class="round">
    <li>
        <div class="entities">
            <div class="ns">
                Organisations</div>
            @Html.RouteLink("Résidences", "TheSaucisseFactory.Residence", new { controller = "Residence" })
            @Html.RouteLink("Bâtiments", "TheSaucisseFactory.Batiment", new { controller = "Batiment" })
            @Html.RouteLink("Appartements", "TheSaucisseFactory.Appartement", new { controller = "Appartement" })
            @Html.RouteLink("Commerces", "TheSaucisseFactory.Commerce", new { controller = "Commerce" })
            <br />
            <br />
            <div class="ns">
                Challenges</div>
            @Html.RouteLink("Challenges", "TheSaucisseFactory.Challenge", new { controller = "Challenge" })
            @Html.RouteLink("Illustrations", "TheSaucisseFactory.Illustration", new { controller = "Illustration" })
            @Html.RouteLink("Gains d'EnergyCoins", "TheSaucisseFactory.GainEnergyCoin", new { controller = "GainEnergyCoin" })
            <br />
            <br />
            <div class="ns">
                Médias</div>
            @Html.RouteLink("Médias, publicités...", "TheSaucisseFactory.Media", new { controller = "Media" })
            <br />
            <br />
            <div class="ns">
                Données brutes</div>
            @Html.RouteLink("Mesures", "TheSaucisseFactory.Mesure", new { controller = "Mesure" })
        </div>
    </li>
</ol>
